    "lint": "eslint './src**/*.{ts,tsx}'",
    "electron-start": "electron .",
    "start-mac": "tsc ./public/main.ts && concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && npm run electron-start\""

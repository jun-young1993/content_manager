{
  "name": "CM",
  "version": "0.1.0",
  "private": true,
  "main": "public/main.js",
  "homepage": "./",
  "dependencies": {
    "@craco/craco": "5.8",
    "@electron/remote": "^2.0.8",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.87",
    "@mui/material": "^5.8.5",
    "@mui/x-data-grid": "^5.12.3",
    "@progress/kendo-data-query": "^1.5.6",
    "@progress/kendo-drawing": "^1.16.3",
    "@progress/kendo-licensing": "^1.2.2",
    "@progress/kendo-react-animation": "^5.3.0",
    "@progress/kendo-react-buttons": "^5.3.0",
    "@progress/kendo-react-data-tools": "^5.3.0",
    "@progress/kendo-react-dateinputs": "^5.3.0",
    "@progress/kendo-react-dropdowns": "^5.3.0",
    "@progress/kendo-react-grid": "^5.3.0",
    "@progress/kendo-react-inputs": "^5.3.0",
    "@progress/kendo-react-intl": "^5.3.0",
    "@progress/kendo-react-treeview": "^5.3.0",
    "@progress/kendo-theme-bootstrap": "^5.4.1",
    "@progress/kendo-theme-default": "^5.4.1",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/uniqid": "^5.3.2",
    "bootstrap": "^5.1.3",
    "craco-alias": "^3.0.1",
    "electron-is-dev": "^2.0.0",
    "install": "^0.13.0",
    "kalend": "^0.14.3",
    "module-alias": "^2.2.2",
    "nedb": "^1.8.0",
    "nedb-promises": "^6.0.3",
    "node-sass": "^7.0.1",
    "npm": "^8.10.0",
    "react": "^17.0.2",
    "react-alert": "^7.0.3",
    "react-dom": "^17.0.2",
    "react-scripts": "^2.1.3",
    "uniqid": "^5.4.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "react-start": "craco start",
    "react-build": "craco build",
    "test": "react-scripts test",
    "eject": "craco eject",
    "start": "tsc ./public/main.ts && concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "npm run react-build && electron-builder",
    "release": "npm run react-build && electron-builder --publish=always",
    "lint": "eslint './src**/*.{ts,tsx}'",
    "electron-start": "electron .",
    "start-mac": "tsc ./public/main.ts && concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && npm run electron-start\"",
    "deploy:win32": "npm run react-build && electron-builder --windows nsis:ia32",
    "deploy:win64": "npm run react-build && electron-builder --windows nsis:x64",
    "deploy:mac": "npm run react-build && electron-builder --macos",
    "electron-pack": "npm run react-build && electron-builder build -c.extraMetadata.main=build/main.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@types/lodash": "^4.14.182",
    "@types/node-sass": "^4.11.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^17.2.0",
    "electron-builder": "^22.14.13",
    "typescript": "^4.6.3",
    "wait-on": "^6.0.1"
  },
  "babel": {
    "presets": [
      "@babel/presets-env",
      "@babel/presets-react",
      "@babel/preset-react"
    ]
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "author": {
    "name": "junyoung kim",
    "email": "juny3738@gmail.com"
  },
  "build": {
    "asar": true,
    "appId": "CM",
    "directories": {
      "buildResources": "src/public",
      "output": "dist/",
      "app": "."
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "productName": "CM",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "target": [
        "default"
      ],
      "icon": "./electron-root/favicon.ico"
    },
    "dmg": {
      "title": "${productName}-Setup-${arch}"
    },
    "win": {
      "target": [
        "nsis",
        "nsis"
      ],
      "icon": "./electron-root/favicon.ico"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "installerIcon": "",
      "allowToChangeInstallationDirectory": false,
      "createDesktopShortcut": true,
      "artifactName": "${productName}-Setup-${arch}.${ext}",
      "installerLanguages": [
        "en_US",
        "ko_KR"
      ],
      "language": "1042"
    }
  }
}
